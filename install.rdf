<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>nosafelinks@moof.ro</em:id>
    <em:type>2</em:type>
    <em:name>Clean up safelinks</em:name>
    <em:version>0.2</em:version>
    <em:creator>Alex Badea</em:creator>
    <em:description>When viewing emails, fix links that were mangled by safelinks.protection.outlook.com.</em:description>
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id> <!-- Thunderbird -->
        <em:minVersion>8.0a1</em:minVersion>
        <em:maxVersion>57.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <em:bootstrap>true</em:bootstrap>
  </Description>
</RDF>
